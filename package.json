{
  "name": "vite-vue-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit false --project tsconfig.prod.json && vite build --emptyOutDir",
    "build:type-check": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "format": "prettier --write src/",
    "clean": "rimraf node_modules package-lock.json"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.7",
    "@tanstack/vue-query": "^5.73.0",
    "@tanstack/vue-query-devtools": "^5.73.0",
    "@vueuse/core": "^10.11.1",
    "browser-image-compression": "^2.0.2",
    "chart.js": "^4.4.8",
    "colorthief": "^2.6.0",
    "jspdf": "^3.0.1",
    "jspdf-autotable": "^5.0.2",
    "lucide-vue-next": "^0.344.0",
    "papaparse": "^5.5.2",
    "pinia": "^2.3.1",
    "vue": "^3.5.13",
    "vue-advanced-cropper": "^2.8.9",
    "vue-chartjs": "^5.3.2",
    "vue-router": "^4.5.0",
    "vue-toastification": "^2.0.0-rc.5"
  },
  "devDependencies": {
    "@eslint/config-array": "^0.20.0",
    "@eslint/object-schema": "^2.1.6",
    "@types/jspdf": "^1.3.3",
    "@types/node": "^20.17.30",
    "@types/papaparse": "^5.3.15",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-vue": "^9.33.0",
    "glob": "^11.0.1",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "supabase": "^1.226.4",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.8.3",
    "vite": "^6.2.6",
    "vitest": "^3.1.1",
    "vue-tsc": "^2.2.8"
  }
}
